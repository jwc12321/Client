    <style name="energy_number_et_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/common_text_left</item>
        <item name="android:background">@null</item>
        <item name="android:inputType">number|numberDecimal</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/appText11</item>
        <item name="android:textColorHint">@color/appText6</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="text_et_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/common_text_left</item>
        <item name="android:background">@null</item>
        <item name="android:maxLength">20</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/appText6</item>
        <item name="android:textColorHint">@color/appText6</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="nickname_et_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/common_text_left</item>
        <item name="android:background">@null</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/appText6</item>
        <item name="android:textColorHint">@color/appText6</item>
        <item name="android:textSize">13sp</item>
    </style>
    <!--猜你喜欢的文字-->
    <style name="like_adapter_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/appText6</item>
    </style>

    <style name="PhotoStyle">
        <item name="android:scaleType">fitXY</item>
        <item name="riv_border_width">1dp</item>
        <item name="riv_border_color">@color/appText3</item>
        <item name="riv_corner_radius">1dp</item>
