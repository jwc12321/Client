        <item name="android:background">@null</item>
        <item name="android:inputType">number|numberDecimal</item>
        <item name="android:maxLength">11</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/appText11</item>
        <item name="android:textColorHint">@color/appText6</item>
