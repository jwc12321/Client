        <item name="android:background">@color/backGround2</item>
    </style>

    <style name="SlsStyleDialog" parent="android:Theme.Holo.Light.Dialog">
        <!--<item name="android:windowContentOverlay">@null</item>-->
        <!--<item name="android:windowBackground">@android:color/transparent</item>-->
        <item name="android:windowMinWidthMajor">90%</item>
        <item name="android:windowMinWidthMinor">90%</item>
    </style>

    <style name="account_detail_left_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/appText6</item>
        <item name="android:layout_marginLeft">@dimen/common_sixteen_margin</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="account_detail_right_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/appText5</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">@dimen/common_sixteen_margin</item>
        <item name="android:layout_marginLeft">@dimen/common_ninety_margin</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">13sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="go_arrow">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:src">@mipmap/arrow_indicator</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/common_eleven_margin</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="common_ok_button_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/title_height</item>
        <item name="android:background">@drawable/login_button_background</item>
        <item name="android:layout_marginLeft">@dimen/common_text_left</item>
        <item name="android:layout_marginRight">@dimen/common_margin_left_right</item>
        <item name="android:enabled">false</item>
        <item name="android:textColor">@color/appText1</item>
    </style>

    <style name="setting_rl_style">
        <item name="android:layout_height">@dimen/title_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/appText1</item>
    </style>

    <style name="setting_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/common_text_left</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/appText5</item>
    </style>

    <style name="persional_im_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/appText6</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/common_text_left</item>
    </style>

    <style name="HeartRatingBarLarge">
        <item name="android:minWidth">10dp</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:progressDrawable">@drawable/rating_heart_large</item>
    </style>

    <style name="BlackNoTitle" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/black</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>

    <style name="pay_item_fl_style">
        <item name="android:layout_height">@dimen/common_forty_four_margin</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/appText1</item>
        <item name="android:paddingLeft">@dimen/common_seventeen_margin</item>
        <item name="android:paddingRight">@dimen/common_seventeen_margin</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="pay_item_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:drawablePadding">@dimen/common_twenty_one_margin</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/appText5</item>
    </style>

    <!-- my loading dialog -->
    <style name="MyDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <!-- popup window 显示消失动画-->
    <style name="PopWindowAnimStyle">
        <item name="android:windowShowAnimation">@anim/dialog_enter</item>
        <!-- 指定显示的动画xml  -->
        <item name="android:windowHideAnimation">@anim/dialog_exit</item>
        <!-- 指定消失的动画xml  -->
    </style>

    <style name="add_address_left_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/appText12</item>
        <item name="android:layout_marginLeft">@dimen/common_sixteen_margin</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_weight">3</item>
    </style>

    <style name="add_address_right_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/appText12</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColorHint">@color/appText2</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginRight">@dimen/common_sixteen_margin</item>
    </style>


    <style name="horizontal_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/appText1</item>
    </style>
    <style name="anim_push_bottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <!--全屏加透明-->
    <style name="TranslucentFullScreenTheme" parent="FullScreenTheme">
        <item name="android:windowBackground">@color/backGround3</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <!--全屏-->
    <style name="FullScreenTheme" parent="AppTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <!-- 对话框样式 -->
    <style name="StyleDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

</resources>
