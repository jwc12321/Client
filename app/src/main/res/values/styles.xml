    <style name="BaseAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/backGround3</item>    <!-- 状态栏 -->
        <item name="colorPrimaryDark">@color/backGround3</item> <!-- toolbar -->
        <item name="colorAccent">@color/colorAccent</item>  <!-- 重点 -->
    </style>
    <style name="AppTheme" parent="@style/BaseAppTheme" />

    <style name="FullscreenTheme" parent="AppTheme">
        <item name="android:actionBarStyle">@style/FullscreenActionBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@mipmap/splash</item>
    </style>

    <style name="FullscreenActionBarStyle" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>
    <!--登录页忘记密码-->
    <style name="login_forget_number_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/appText4</item>
        <item name="android:textSize">11sp</item>
        <item name="android:padding">14dp</item>
    </style>


    <style name="title_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/common_forty_four_margin</item>
        <item name="android:textColor">@color/appText5</item>
        <item name="android:textSize">17sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="title_text_right_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/common_forty_four_margin</item>
        <item name="android:textColor">@color/appText5</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingRight">@dimen/common_eleven_margin</item>
        <item name="android:paddingLeft">@dimen/common_eleven_margin</item>
    </style>


    <!--title最左边的返回-->
    <style name="title_left_back_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/common_forty_four_margin</item>
        <item name="android:src">@mipmap/return_gray</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">@dimen/common_eleven_margin</item>
        <item name="android:paddingRight">@dimen/common_eleven_margin</item>
    </style>
    <!--title最右边图片-->
    <style name="title_right_back_style">
        <item name="android:layout_width">@dimen/common_forty_four_margin</item>
        <item name="android:layout_height">@dimen/common_forty_four_margin</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@null</item>
        <item name="android:padding">@dimen/common_eleven_margin</item>
    </style>

    <!--首页底部LinearLayout-->
    <style name="bottom_RelativeLayout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>
    <!--首页底部图片-->
    <style name="bottom_imageView_style">
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_width">22dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <!--首页底部文字-->
    <style name="bottom_textView_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/bottom_text_select</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginBottom">6dp</item>
    </style>

    <style name="phone_number_et_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/common_text_left</item>
        <item name="android:background">@null</item>
        <item name="android:inputType">phone</item>
        <item name="android:maxLength">11</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/appText6</item>
        <item name="android:textColorHint">@color/appText6</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="password_et_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/common_text_left</item>
        <item name="android:background">@null</item>
        <item name="android:inputType">phone</item>
        <item name="android:maxLength">23</item>
